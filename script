!/bin/bash

echo "Установка пакета React..."
yarn add react

echo "Установка пакета React-Dom..."
yarn add react-dom

echo "Установка пакета TypeScript..."
yarn add typescript

echo "Установка пакета EslintTypeScript"
yarn add @typescript-eslint/types

echo "Установка пакета Eslint"
yarn add eslint

echo "Установка пакета Babel"
yarn add @babel/parser

echo "Установка зависимостей завершена!"
